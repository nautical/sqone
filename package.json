{
  "name": "sqone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "precommit": "lint-staged",
    "fix:assets": "npm run prettier --write",
    "fix:code": "npm run test:code --fix",
    "fix": "npm run fix:code && npm run fix:assets",
    "prettier": "prettier --ignore-path .gitignore  \"**/*.{json,md,scss}\"",
    "test:assets": "npm run prettier --list-different",
    "test:code": "eslint --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\"",
    "test": "npm run test:assets && npm run test:code"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "debug": "2.6.9",
    "express": "4.15.5",
    "morgan": "1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "2.4.5"
  },
  "devDependencies": {
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "nodemon": "1.17.2",
    "prettier": "1.11.1"
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint --fix", "git add"],
    "*.{json,md,scss}": ["prettier --write", "git add"]
  }
}
